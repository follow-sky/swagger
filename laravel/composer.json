{
    "name": "followSky/swagger",
    "description": "laravel6 swagger",
    "type": "project",
    "license": "MIT",
    "authors" : [{
			"name" : "followSky",
			"email" : "admin@admin.com"
		}
	],
	"require" : {
		"php" : ">=7.0.0",
		"illuminate/support" : "6.*",
		"illuminate/view" : "6.*",
		"illuminate/config" : "6.*",
		"illuminate/console" : "6.*",
		"zircote/swagger-php" : "2.*"
	},
	"require-dev" : {
		"mockery/mockery" : "0.9.*",
		"phpunit/phpunit" : "~6.0"
	},
	"extra" : {
		"laravel": {
			"providers": [
				"Swagger6\\Laravel\\SwaggerServiceProvider"
			]
		}
	},
	"autoload": {
		"psr-4": {
			"Swagger\\Laravel\\": "src/"
		}
	},
	"autoload-dev" : {
		"files" : [
			"tests/TestCase.php"
		]
	},
	"minimum-stability" : "stable"
}
